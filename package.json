{
  "name": "@asanrom/async-tools",
  "version": "1.0.0",
  "description": "Collection of tools to work with async functions.",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "homepage": "https://github.com/AgustinSRG/async-tools",
  "repository": {
    "type": "git",
    "url": "https://github.com/AgustinSRG/async-tools"
  },
  "scripts": {
    "prebuild": "npm run lint",
    "compile": "tsc --pretty --declaration && webpack",
    "build": "rimraf dist && npm run compile",
    "lint": "eslint --fix src/**/*.ts",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'tests/**/*.ts'",
    "gendoc": "typedoc --out docs ./src"
  },
  "files": [
    "dist/"
  ],
  "author": {
    "name": "Agustin San Roman",
    "email": "agustinsanromanguzman@gmail.com",
    "url": "https://github.com/AgustinSRG"
  },
  "engines": {
    "node": ">= 12.0.0"
  },
  "pre-commit": [
    "lint"
  ],
  "devDependencies": {
    "@types/chai": "4.3.3",
    "@types/mocha": "9.1.1",
    "@types/node": "18.7.18",
    "@typescript-eslint/eslint-plugin": "5.38.0",
    "@typescript-eslint/parser": "5.38.0",
    "chai": "4.3.6",
    "eslint": "8.23.1",
    "mocha": "10.0.0",
    "rimraf": "3.0.2",
    "ts-loader": "9.4.1",
    "ts-node": "10.9.1",
    "typedoc": "0.23.15",
    "typescript": "4.8.3",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0"
  },
  "license": "MIT"
}
